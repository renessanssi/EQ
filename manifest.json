{
  "manifest_version": 3,
  "name": "EQ",
  "version": "1.0",
  "description": "Open Source EQ",
  "permissions": ["scripting", "activeTab", "storage"],
  "action": {
    "default_popup": "popup/index.html",
    "default_title": "EQ",
    "default_icon": {
      "16": "assets/icon128.png",
      "32": "assets/icon128.png",
      "48": "assets/icon128.png",
      "128": "assets/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icon128.png",
    "32": "assets/icon128.png",
    "48": "assets/icon128.png",
    "128": "assets/icon128.png"
  },
  "host_permissions": [
  "http://*/*",
  "https://*/*"
]
}
