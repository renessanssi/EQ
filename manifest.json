{
  "manifest_version": 3,
  "name": "EQ",
  "version": "1.0",
  "description": "Open Source EQ",
  "permissions": ["scripting", "activeTab", "storage"],
  "action": {
    "default_popup": "popup/index.html",
    "default_title": "EQ",
    "default_icon": "./assets/icon.png"
  },
  "host_permissions": ["<all_urls>"]
}